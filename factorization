#include <bits/stdc++.h>
Using namespace std;
void primeFactors(int n) 
{ 
    // Print the number of 2s that divide n 
while (n % 2 == 0) 
    { 
Cout<< 2 << “ “; 
    N = n/2; 
   } 
for (int I = 3; I <= sqrt(n); I = I + 2) 
 { 
  while (n % I == 0) 
 { 
Cout<< I << “ “; 
  N = n/I; 
    } 
    } 
   // is a prime number greater than 2 
    If (n > 2) 
Cout<< n << “ “; 
} 
/* Driver code */
int main() 
{ 
int n = 315; 
primeFactors(n); 
return 0; 
}
